{
    "name": "synthetic",
    "executor": {
        "max_processes": 1
    },
    "transformer": {
        "ps_type": "redis",
        "ps_file_dir": "/tmp/file-cache",
        "ps_redis_addr": "localhost:59465",
        "ps_extract_target": false
    },
    "filter": {
        "filter_type": "object-size",
        "filter_min_size": 5000,
        "filter_max_size": null
    },
    "run": {
        "log_file_level": "INFO",
        "log_file_name": "log.txt",
        "log_level": "WORK",
        "task_record_file_name": "tasks.json",
        "run_dir_format": "/home/cc/escience24-taps-analysis/runs/data/{name}/process-pool/{timestamp}"
    },
    "workflow": {
        "structure": "bag",
        "task_count": 10,
        "task_data_bytes": 10000,
        "task_sleep": 0.0,
        "bag_max_running": 1
    }
}